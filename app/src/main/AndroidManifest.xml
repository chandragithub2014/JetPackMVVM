<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.example.jetpackmvvmdemos">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
            android:name=".MyRetroApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:networkSecurityConfig="@xml/network_security_config"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".workmanagersample.views.WorkMangerSampleActivity"></activity>
        <activity android:name=".Launcher.view.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".dataBindingActivity.views.SimpleDataBindingActivity" />
        <activity android:name=".databindingfragment.views.SimpleDataBindingFragmentActivity" />
        <activity android:name=".dataBindingAdapter.views.DataBindingListActivity" />
        <activity android:name=".network.view.NetworkActivity" />
        <activity android:name=".pagingJetPack.view.PagingRoomDataBaseActivity" />
        <activity android:name=".dataBindingRoomDB.views.RoomDataBaseActivity" />
        <activity android:name=".LifeCycleAwares.view.LifeCycleAwareActivity" />
        <activity android:name=".navigationJetPack.views.NavigationMainActivity" />
        <activity android:name=".workmanager.view.WorkManagerActivity" />
    </application>

</manifest>